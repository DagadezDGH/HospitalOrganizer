<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="view" prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/Clasico.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <rowConstraints>
      <RowConstraints />
      <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints minWidth="10.0" />
      <ColumnConstraints />
      <ColumnConstraints hgrow="ALWAYS" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <children>
      <Label text="Proveedor:">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin>
      </Label>
      <ComboBox fx:id="proveedorCombo" styleClass="blue" stylesheets="@../css/Clasico.css" GridPane.columnIndex="1">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin>
      </ComboBox>
      <Label layoutX="10.0" layoutY="17.0" text="Fecha entrada:" GridPane.columnIndex="2">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin>
      </Label>
      <ComboBox fx:id="fechaEntradaCombo" styleClass="blue" stylesheets="@../css/Clasico.css" GridPane.columnIndex="3">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin>
      </ComboBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="4">
         <children>
            <Button fx:id="informeButton" mnemonicParsing="false" onAction="#OnInformeAction" styleClass="buttons" stylesheets="@../css/Clasico.css">
               <graphic>
                  <ImageView pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/informex48.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="volverButton" mnemonicParsing="false" onAction="#OnVolverAction" style="-fx-background-color: lightgrey;" styleClass="buttons" stylesheets="@../css/Clasico.css">
               <graphic>
                  <ImageView pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/home_icon-iconsx48.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin>
      </HBox>
      <TableView fx:id="entradaArticuloTable" styleClass="blue" stylesheets="@../css/Clasico.css" GridPane.columnSpan="5" GridPane.rowIndex="1">
        <columns>
          <TableColumn fx:id="nombreArticuloColumn" prefWidth="75.0" style="-fx-background-color: lightgrey;" text="Articulo" />
          <TableColumn fx:id="cantidadColumn" prefWidth="75.0" style="-fx-background-color: lightgrey;" text="Cantidad" />
            <TableColumn fx:id="caducidadColumn" prefWidth="75.0" style="-fx-background-color: lightgrey;" text="Caducidad" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin>
      </TableView>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <styleClass>
      <String fx:value="blue" />
      <String fx:value="green" />
   </styleClass>
</GridPane>
